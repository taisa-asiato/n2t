/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not ( in=sel, out=nsel );
    And ( a=a, b=nsel, out=anda );
    And ( b=b, a=sel, out=andb );
    Or ( a=anda, b=andb, out=out );
}
